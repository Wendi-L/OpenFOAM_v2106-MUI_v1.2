+sinclude $(RULES)/mplib$(WM_MPLIB) << This is new 
+sinclude $(GENERAL_RULES)/MUI << This is new 
+
+EXE_INC = \
+    $(PFLAGS) \ << This is new 
+    $(PINC) \ << This is new 
+    ${MUI_INC} \ << This is new 
+    -I$(LIB_SRC)/finiteVolume/lnInclude \
+    -I$(LIB_SRC)/meshTools/lnInclude
+
+EXE_LIBS = \
+    $(PLIBS) \ << This is new 
+    -lfiniteVolume \
+    -lmeshTools
+
